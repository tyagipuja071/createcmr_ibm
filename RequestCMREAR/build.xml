<project name="Build" default="build" basedir=".">

    <target name="build"> 
        <getProjectData basedir="${basedir}"  />
        <echo message="${ant.project.name} build: project=${projectName} nature=${natureName} ... "  />
        <ant antfile="${projectBuildFile}" target="buildPROJECT" >  
            <property name="projectName" value="CreateCMREAR"/>  
            <property name="nature" value="${natureName}"/>  
            <property name="output" value="" />
            <property name="source" value="" />
        </ant>
    </target>
</project>
